function Set (list)
   local set = {}
   for _, l in ipairs(list) do set[l] = true end
   return set
end
return Set {
    "0000800482000000080002408100003011005040800000000000000050000010",
    "0001006280000004004001008004002000000040880400000000182800004509",
    "0000000080400001004400008000001000020005920100000080400201000001" -- once you get the user's hashed hwid and decoded it put it here
 
}
